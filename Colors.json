{
    "name": "Neon Pro - Dark Canvas Fix",
    "dataColors": [
        "#4895EF", 
        "#4CC9F0", 
        "#666666", 
        "#F72585", 
        "#00F5D4", 
        "#E0E0E0"
    ],
    "visualStyles": {
        "*": {
            "*": {
                "background": [
                    {
                        "show": true,
                        "color": { "solid": { "color": "#2D2D30" } },
                        "transparency": 0
                    }
                ],
                "border": [
                    {
                        "show": true,
                        "color": { "solid": { "color": "#3E3E42" } },
                        "radius": 10
                    }
                ],
                "title": [
                    {
                        "show": true,
                        "fontColor": { "solid": { "color": "#F2F2F2" } },
                        "background": { "solid": { "color": "#2D2D30" } },
                        "fontSize": 14,
                        "fontFamily": "Segoe UI Semibold"
                    }
                ]
            }
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": { "solid": { "color": "#1A1C1E" } },
                        "transparency": 0
                    }
                ],
                "outspace": [
                    {
                        "color": { "solid": { "color": "#1A1C1E" } },
                        "transparency": 0
                    }
                ]
            }
        }
    }
}